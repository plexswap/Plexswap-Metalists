{
  "name": "metalists",
  "version": "1.0.1",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "checksum:plexswap-default": "yarn build && node ./dist checksum plexswap-default",
    "generate:plexswap-default": "yarn test --list=plexswap-default && yarn build && node ./dist generate plexswap-default",
    "makelist:plexswap-default": "yarn checksum:plexswap-default && yarn generate:plexswap-default",
    "checksum:plexswap-extended": "yarn build && node ./dist checksum plexswap-extended",
    "generate:plexswap-extended": "yarn test --list=plexswap-extended && yarn build && node ./dist generate plexswap-extended",
    "makelist:plexswap-extended": "yarn checksum:plexswap-extended && yarn generate:plexswap-extended",
    "checksum:plexswap-mmbsc": "yarn build && node ./dist checksum plexswap-mmbsc",
    "generate:plexswap-mmbsc": "yarn test --list=plexswap-mmbsc && yarn build && node ./dist generate plexswap-mmbsc",
    "makelist:plexswap-mmbsc": "yarn checksum:plexswap-mmbsc && yarn generate:plexswap-mmbsc",
    "checksum:plexswap-onramp": "yarn build && node ./dist checksum plexswap-onramp",
    "generate:plexswap-onramp": "yarn test --list=plexswap-onramp && yarn build && node ./dist generate plexswap-onramp",
    "makelist:plexswap-onramp": "yarn checksum:plexswap-onramp && yarn generate:plexswap-onramp",
    "checksum:coingecko": "yarn build && node ./dist checksum coingecko",
    "generate:coingecko": "yarn test --list=coingecko && yarn build && node ./dist generate coingecko",
    "makelist:coingecko": "yarn checksum:coingecko && yarn generate:coingecko",
    "checksum:cmc": "yarn build && node ./dist checksum cmc",
    "generate:cmc": "yarn test --list=cmc && yarn build && node ./dist generate cmc",
    "makelist:cmc": "yarn checksum:cmc && yarn generate:cmc",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "fetch:coingecko": "yarn build && node ./dist fetch coingecko",
    "fetch:cmc": "yarn build && node ./dist fetch cmc",
    "test": "jest",
    "ci-check": "yarn build && node ./dist ci-check",
    "postinstall": "yarn run typechain",
    "typechain": "typechain --out-dir src/utils/abi/types --target=ethers-v5 \"src/utils/abi/*.json\""
  },
  "dependencies": {
    "@ethersproject/address": "^5.1.0",
    "@typechain/ethers-v5": "^9.0.0",
    "ajv": "6.12.2",
    "aptos": "^1.3.16",
    "axios": "^0.26.0",
    "ethers": "^5.5.4",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "lodash": "^4.17.21",
    "slugify": "^1.6.0",
    "tslib": "^2.4.0",
    "typechain": "^7.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.182",
    "@types/node": "^15.0.2",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "rollup": "^2.47.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.6.3"
  }
}
